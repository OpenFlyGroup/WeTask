{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps",
  "monorepo": true,
  "root": "apps",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "projects": {
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "users-service": {
      "type": "application",
      "root": "apps/users-service",
      "entryFile": "main",
      "sourceRoot": "apps/users-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users-service/tsconfig.app.json"
      }
    },
    "teams-service": {
      "type": "application",
      "root": "apps/teams-service",
      "entryFile": "main",
      "sourceRoot": "apps/teams-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/teams-service/tsconfig.app.json"
      }
    },
    "boards-service": {
      "type": "application",
      "root": "apps/boards-service",
      "entryFile": "main",
      "sourceRoot": "apps/boards-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/boards-service/tsconfig.app.json"
      }
    },
    "tasks-service": {
      "type": "application",
      "root": "apps/tasks-service",
      "entryFile": "main",
      "sourceRoot": "apps/tasks-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tasks-service/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    }
  }
}
